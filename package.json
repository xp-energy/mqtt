{
  "name": "mqtt",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xp-energy/mqtt"
  },
  "scripts": {
    "commit": "git cz",
    "prepare": "husky install",
    "release": "semantic-release --no-ci",
    "test": "jest --coverage"
